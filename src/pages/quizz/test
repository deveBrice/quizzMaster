temp = 0;
flag = boolean = true;
var tab = new Array(10);
tab[0] = Math.floor(Math.random() * Math.floor(10))

for(var i = 0; i<tab.length; i++){
  
  temp = Math.floor(Math.random() * Math.floor(10));

  for(var j = 0; j<tab.length; j++){
    
    if(temp != tab[j]){
      
       flag = true;
      
    }else {
        
      flag = false;
      break;
    }
  }
  
  if(flag === true){
   
   tab[i] = temp;
  
  }else {
    i--;
    }
  }

var i=0;
while(i<tab.length -1){
  i++
